   "source": [
    "f=open(\"para.txt\",\"r\")\n",
    "f_out=open(\"para_word\",\"w\")\n",
    "for line in f:\n",
    "    tokens=line.split(\" \")\n",
    "    f_out.write(\"wordcount:\"+\" \"  + str(len(tokens)) +\" \" +line)\n",
    "f.close()  \n",
    "f_out.close()"
   ]
